			ТОГДА Договоры_ат.Ссылка
		ИНАЧЕ Договоры_ат.Родитель
	КОНЕЦ КАК ОсновнойДоговор,
	Договоры_ат.ВидДоговора КАК ВидДоговора
ПОМЕСТИТЬ Договоры
ИЗ
	Справочник.Договоры_ат КАК Договоры_ат
ГДЕ
	НЕ Договоры_ат.ПометкаУдаления
	И НЕ Договоры_ат.ДатаНачала = ДАТАВРЕМЯ(1, 1, 1)
	И Договоры_ат.Владелец.Клиент
{ГДЕ
	(Договоры_ат.ДатаНачала &lt;= &amp;КонецПериода),
	((Договоры_ат.ДатаРасторжения = ДАТАВРЕМЯ(1, 1, 1)
			ИЛИ Договоры_ат.ДатаРасторжения &gt;= &amp;КонецПериода)
			И (Договоры_ат.СрокДоговора = ДАТАВРЕМЯ(1, 1, 1)
				ИЛИ Договоры_ат.СрокДоговора &gt;= &amp;КонецПериода
				ИЛИ Договоры_ат.Автопролонгация))}
;

////////////////////////////////////////////////////////////////////////////////
