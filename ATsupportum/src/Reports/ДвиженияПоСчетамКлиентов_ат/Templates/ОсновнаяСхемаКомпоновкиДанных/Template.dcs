			<dcsat:name>ПериодГруппировки</dcsat:name>
			<dcsat:expression>"Обороты за " + ВЫБОР   
КОГДА &amp;Периодичность = 6 
   ТОГДА Формат(ФактПериодГруппировки, "ДФ=dd.MM.yy")  
   КОГДА &amp;Периодичность = 7
   ТОГДА Формат(НачалоПериода(ФактПериодГруппировки, "Неделя"), "ДФ=dd.MM.yy")  + " - " + Формат(КонецПериода(ФактПериодГруппировки, "Неделя"), "ДФ=dd.MM.yy") 
   КОГДА &amp;Периодичность = 8
   ТОГДА Формат(НачалоПериода(ФактПериодГруппировки, "Декада"), "ДФ=dd.MM.yy")  + " - " + Формат(КонецПериода(ФактПериодГруппировки, "Декада"), "ДФ=dd.MM.yy") 
   КОГДА &amp;Периодичность = 9
   ТОГДА Формат(ФактПериодГруппировки, "ДФ = 'MMMM yy'")
   КОГДА &amp;Периодичность = 10
   ТОГДА 
      ВЫБОР
         КОГДА Квартал(ФактПериодГруппировки) = 1 
         ТОГДА Формат(ФактПериодГруппировки, "ДФ = '""1 кв."" yy'")
         КОГДА Квартал(ФактПериодГруппировки) = 2 
         ТОГДА Формат(ФактПериодГруппировки, "ДФ = '""2 кв."" yy'")
         КОГДА Квартал(ФактПериодГруппировки) = 3 
         ТОГДА Формат(ФактПериодГруппировки, "ДФ = '""3 кв."" yy'")
         КОГДА Квартал(ФактПериодГруппировки) = 4 
         ТОГДА Формат(ФактПериодГруппировки, "ДФ = '""4 кв."" yy'")
      КОНЕЦ
   КОГДА &amp;Периодичность = 11
   ТОГДА 
      ВЫБОР
         КОГДА Квартал(ФактПериодГруппировки) &lt; 2 
         ТОГДА Формат(ФактПериодГруппировки, "ДФ = '""1 полугодие"" yy'")
         ИНАЧЕ Формат(ФактПериодГруппировки, "ДФ = '""2 полугодие"" yy'")
      КОНЕЦ
   КОГДА &amp;Периодичность = 12
   ТОГДА Формат(ФактПериодГруппировки, "ДФ = 'yyyy'")
КОНЕЦ</dcsat:expression>
