		<parameter xmlns:dcsat="http://v8.1c.ru/8.1/data-composition-system/area-template" xsi:type="dcsat:ExpressionAreaTemplateParameter">
			<dcsat:name>ПериодГруппировки</dcsat:name>
			<dcsat:expression>"Обороты за " + ВЫБОР   
КОГДА &amp;Периодичность = 6 
   ТОГДА Формат(ОперПериодГруппировки, "ДФ=dd.MM.yy")  
   КОГДА &amp;Периодичность = 7
   ТОГДА Формат(НачалоПериода(ОперПериодГруппировки, "Неделя"), "ДФ=dd.MM.yy")  + " - " + Формат(КонецПериода(ОперПериодГруппировки, "Неделя"), "ДФ=dd.MM.yy") 
   КОГДА &amp;Периодичность = 8
   ТОГДА Формат(НачалоПериода(ОперПериодГруппировки, "Декада"), "ДФ=dd.MM.yy")  + " - " + Формат(КонецПериода(ОперПериодГруппировки, "Декада"), "ДФ=dd.MM.yy") 
   КОГДА &amp;Периодичность = 9
   ТОГДА Формат(ОперПериодГруппировки, "ДФ = 'MMMM yy'")
   КОГДА &amp;Периодичность = 10
   ТОГДА 
      ВЫБОР
         КОГДА Квартал(ОперПериодГруппировки) = 1 
         ТОГДА Формат(ОперПериодГруппировки, "ДФ = '""1 кв."" yy'")
         КОГДА Квартал(ОперПериодГруппировки) = 2 
         ТОГДА Формат(ОперПериодГруппировки, "ДФ = '""2 кв."" yy'")
         КОГДА Квартал(ОперПериодГруппировки) = 3 
         ТОГДА Формат(ОперПериодГруппировки, "ДФ = '""3 кв."" yy'")
         КОГДА Квартал(ОперПериодГруппировки) = 4 
         ТОГДА Формат(ОперПериодГруппировки, "ДФ = '""4 кв."" yy'")
      КОНЕЦ
   КОГДА &amp;Периодичность = 11
   ТОГДА 
      ВЫБОР
         КОГДА Квартал(ОперПериодГруппировки) &lt; 2 
         ТОГДА Формат(ОперПериодГруппировки, "ДФ = '""1 полугодие"" yy'")
         ИНАЧЕ Формат(ОперПериодГруппировки, "ДФ = '""2 полугодие"" yy'")
      КОНЕЦ
   КОГДА &amp;Периодичность = 12
   ТОГДА Формат(ОперПериодГруппировки, "ДФ = 'yyyy'")
КОНЕЦ</dcsat:expression>
