////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ОстаткиНаНачало.Субконто1 КАК ОсновнойДоговор,
	ЕСТЬNULL(ОстаткиНаНачало.СуммаОстаток, 0) КАК ФактОбщийНачальныйОстаток,
	ЕСТЬNULL(ОстаткиНаНачало.СуммаОстатокДт, 0) КАК ФактОбщийНачальныйОстатокДт,
	ЕСТЬNULL(ОстаткиНаНачало.СуммаОстатокКт, 0) КАК ФактОбщийНачальныйОстатокКт
ПОМЕСТИТЬ ФактНачальныеОстатки
ИЗ
	РегистрБухгалтерии.Клиентский_ат.Остатки({(&amp;ОстаткиНачалоПериода)}, Счет = ЗНАЧЕНИЕ(ПланСчетов.Клиентский_ат.Фактический), , ) КАК ОстаткиНаНачало
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ОстаткиНаКонец.Субконто1 КАК ОсновнойДоговор,
	ЕСТЬNULL(ОстаткиНаКонец.СуммаОстаток, 0) КАК ФактОбщийКонечныйОстаток,
	ЕСТЬNULL(ОстаткиНаКонец.СуммаОстатокДт, 0) КАК ФактОбщийКонечныйОстатокДт,
	ЕСТЬNULL(ОстаткиНаКонец.СуммаОстатокКт, 0) КАК ФактОбщийКонечныйОстатокКт
ПОМЕСТИТЬ ФактКонечныеОстатки
ИЗ
	РегистрБухгалтерии.Клиентский_ат.Остатки({(&amp;КонецПериода)}, Счет = ЗНАЧЕНИЕ(ПланСчетов.Клиентский_ат.Фактический), , ) КАК ОстаткиНаКонец
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	Клиентский_атОстаткиИОбороты.Субконто1 КАК ОсновнойДоговор,
	Клиентский_атОстаткиИОбороты.Период КАК ФактПериод,
	Клиентский_атОстаткиИОбороты.Регистратор КАК ФактРегистратор,
	Клиентский_атДвиженияССубконто.Субконто2 КАК ФактДокумент,
	Клиентский_атДвиженияССубконто.НомерСтроки КАК ФактНомерСтроки,
	ЕСТЬNULL(Клиентский_атОстаткиИОбороты.СуммаНачальныйОстаток, 0) КАК ФактНачальныйОстаток,
	ЕСТЬNULL(Клиентский_атОстаткиИОбороты.СуммаНачальныйОстатокДт, 0) КАК ФактНачальныйОстатокДт,
	ЕСТЬNULL(Клиентский_атОстаткиИОбороты.СуммаНачальныйОстатокКт, 0) КАК ФактНачальныйОстатокКт,
	ЕСТЬNULL(Клиентский_атОстаткиИОбороты.СуммаКонечныйОстаток, 0) КАК ФактКонечныйОстаток,
	ЕСТЬNULL(Клиентский_атОстаткиИОбороты.СуммаКонечныйОстатокДт, 0) КАК ФактКонечныйОстатокДт,
	ЕСТЬNULL(Клиентский_атОстаткиИОбороты.СуммаКонечныйОстатокКт, 0) КАК ФактКонечныйОстатокКт,
	ЕСТЬNULL(Клиентский_атОстаткиИОбороты.СуммаОборот, 0) КАК ФактОборот,
	ЕСТЬNULL(Клиентский_атОстаткиИОбороты.СуммаОборотДт, 0) КАК ФактОборотДт,
	ЕСТЬNULL(Клиентский_атОстаткиИОбороты.СуммаОборотКт, 0) КАК ФактОборотКт,
	ВЫБОР
		КОГДА &amp;Периодичность = 6
			ТОГДА НАЧАЛОПЕРИОДА(Клиентский_атДвиженияССубконто.Период, ДЕНЬ)
		КОГДА &amp;Периодичность = 7
			ТОГДА НАЧАЛОПЕРИОДА(Клиентский_атДвиженияССубконто.Период, НЕДЕЛЯ)
		КОГДА &amp;Периодичность = 8
			ТОГДА НАЧАЛОПЕРИОДА(Клиентский_атДвиженияССубконто.Период, ДЕКАДА)
		КОГДА &amp;Периодичность = 9
			ТОГДА НАЧАЛОПЕРИОДА(Клиентский_атДвиженияССубконто.Период, МЕСЯЦ)
		КОГДА &amp;Периодичность = 10
			ТОГДА НАЧАЛОПЕРИОДА(Клиентский_атДвиженияССубконто.Период, КВАРТАЛ)
		КОГДА &amp;Периодичность = 11
			ТОГДА НАЧАЛОПЕРИОДА(Клиентский_атДвиженияССубконто.Период, ПОЛУГОДИЕ)
		КОГДА &amp;Периодичность = 12
			ТОГДА НАЧАЛОПЕРИОДА(Клиентский_атДвиженияССубконто.Период, ГОД)
	КОНЕЦ КАК ФактПериодГруппировки
ПОМЕСТИТЬ ФактБаланс
ИЗ
	РегистрБухгалтерии.Клиентский_ат.ОстаткиИОбороты({(&amp;НачалоПериода)}, {(&amp;КонецПериода)}, Запись, Движения, Счет = ЗНАЧЕНИЕ(ПланСчетов.Клиентский_ат.Фактический), , ) КАК Клиентский_атОстаткиИОбороты
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрБухгалтерии.Клиентский_ат.ДвиженияССубконто({(&amp;НачалоПериода)}, {(&amp;КонецПериода)}, , , ) КАК Клиентский_атДвиженияССубконто
		ПО Клиентский_атОстаткиИОбороты.Период = Клиентский_атДвиженияССубконто.Период
			И Клиентский_атОстаткиИОбороты.Регистратор = Клиентский_атДвиженияССубконто.Регистратор
			И Клиентский_атОстаткиИОбороты.НомерСтроки = Клиентский_атДвиженияССубконто.НомерСтроки
;

