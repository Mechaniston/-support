    </notDefaultUseAlwaysAttributes>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ //РАЗЛИЧНЫЕ
	Задание_ат.Ссылка КАК Задание,
	Задание_ат.Номер КАК Номер,
	Задание_ат.Дата КАК Дата,
	Задание_ат.Тезис КАК Тезис,
	Задание_ат.СодержаниеРабот КАК СодержаниеРабот,
	ВЫБОР
		КОГДА ВЫРАЗИТЬ(ЕСТЬNULL(ИсторияКомментариев_атСрезПоследних.Комментарий, &quot;&quot;) КАК Строка(5)) = &quot;&quot;
			ТОГДА ЛОЖЬ
			ИНАЧЕ ИСТИНА
		КОНЕЦ КАК ЕстьКомментарий,
	ВЫРАЗИТЬ(ИсторияКомментариев_атСрезПоследних.Комментарий КАК СТРОКА(1024)) КАК Комментарий,
	ВЫРАЗИТЬ(РодительскиеЗаявки.Предок КАК Документ.Заявка_ат) КАК РодительскаяЗаявка,
	ВЫРАЗИТЬ(РодительскиеЗаявки.Предок КАК Документ.Заявка_ат).Проект КАК РодительскаяЗаявка_Проект,
	ВЫРАЗИТЬ(РодительскиеЗаявки.Предок КАК Документ.Заявка_ат).Клиент КАК РодительскаяЗаявка_Клиент,
	СвойстваЗаданий_ат.Исполнитель КАК Исполнитель,
	СвойстваЗаданий_ат.ПланируемаяДатаНачала КАК ПланируемаяДатаНачала,
	СвойстваЗаданий_ат.ПланируемаяДатаОкончания КАК ПланируемаяДатаОкончания,
	СвойстваЗаданий_ат.ПланируемаяДлительность КАК ПланируемаяДлительность,
	ВЫБОР
		КОГДА ЕСТЬNULL(СвойстваЗаданий_ат.ПланируемоеВремяВыполнения, 0) = 0
			ТОГДА 0
			ИНАЧЕ (ВЫРАЗИТЬ(СвойстваЗаданий_ат.ПланируемоеВремяВыполнения / 60 - 0.5 КАК ЧИСЛО(8, 0)))
				+ (СвойстваЗаданий_ат.ПланируемоеВремяВыполнения - (ВЫРАЗИТЬ(СвойстваЗаданий_ат.ПланируемоеВремяВыполнения / 60 - 0.5 КАК ЧИСЛО(8, 0))) * 60) / 100
		КОНЕЦ КАК ПланируемоеВремяВыполнения,
	СвойстваЗаданий_ат.ФактическаяДатаНачала КАК ФактическаяДатаНачала,
	СвойстваЗаданий_ат.ФактическаяДатаОкончания КАК ФактическаяДатаОкончания,
	СвойстваЗаданий_ат.ФактическаяДлительность КАК ФактическаяДлительность,
	ВЫБОР
		КОГДА ЕСТЬNULL(СвойстваЗаданий_ат.ФактическоеВремяВыполнения, 0) = 0
			ТОГДА 0
			ИНАЧЕ (ВЫРАЗИТЬ(СвойстваЗаданий_ат.ФактическоеВремяВыполнения / 60 - 0.5 КАК ЧИСЛО(8, 0)))
				+ (СвойстваЗаданий_ат.ФактическоеВремяВыполнения - (ВЫРАЗИТЬ(СвойстваЗаданий_ат.ФактическоеВремяВыполнения / 60 - 0.5 КАК ЧИСЛО(8, 0))) * 60) / 100
		КОНЕЦ КАК ФактическоеВремяВыполнения,
	ЕСТЬNULL(ИсторияСтатусовЗаданий_атСрезПоследних.Статус, ЗНАЧЕНИЕ(Перечисление.СтатусыЗаданий_ат.ПустаяСсылка)) КАК Статус,
	ИсторияСтатусовЗаданий_атСрезПоследних.Период КАК ДатаСтатуса,
	ИсторияСтатусовРодительскихЗаданий.РодительскоеЗаданиеЗакрыто,
	ВЫБОР
		КОГДА СвойстваЗаданий_ат.Исполнитель = &amp;ПользовательФильтраПерсональные
			ТОГДА ИСТИНА
			ИНАЧЕ ЛОЖЬ
		КОНЕЦ КАК ЯИсполнитель,
	ВЫБОР
		КОГДА ИсторияСтатусовЗаданий_атСрезПоследних.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаданий_ат.ВРаботе)
			ТОГДА 0
			ИНАЧЕ ВЫБОР
				КОГДА ИсторияСтатусовЗаданий_атСрезПоследних.Статус ЕСТЬ NULL
						ИЛИ ИсторияСтатусовЗаданий_атСрезПоследних.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаданий_ат.Планируемое)
						ИЛИ ИсторияСтатусовЗаданий_атСрезПоследних.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаданий_ат.ВОчереди)
						ИЛИ ИсторияСтатусовЗаданий_атСрезПоследних.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаданий_ат.КВозобновлению)
					ТОГДА 1
					ИНАЧЕ 2
				КОНЕЦ
		КОНЕЦ КАК ПриоритетПоСтатусу,
	ВЫБОР
		КОГДА ИсторияСтатусовЗаданий_атСрезПоследних.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаданий_ат.Прервано)
				ИЛИ ИсторияСтатусовЗаданий_атСрезПоследних.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаданий_ат.Выполнено)
				ИЛИ ИсторияСтатусовЗаданий_атСрезПоследних.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаданий_ат.Отменено)
			ТОГДА СвойстваЗаданий_ат.ФактическаяДатаОкончания
			ИНАЧЕ СвойстваЗаданий_ат.ПланируемаяДатаНачала
		КОНЕЦ КАК ЗначениеСортировки
ИЗ
	Документ.Задание_ат КАК Задание_ат
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.СвязиОбъектов_ат КАК РодительскиеЗаявки
			ПО Задание_ат.Ссылка = РодительскиеЗаявки.Объект
				И (РодительскиеЗаявки.Предок ССЫЛКА Документ.Заявка_ат)
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.СвойстваЗаданий_ат КАК СвойстваЗаданий_ат
			ПО Задание_ат.Ссылка = СвойстваЗаданий_ат.Ссылка
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ИсторияСтатусовЗаданий_ат.СрезПоследних КАК ИсторияСтатусовЗаданий_атСрезПоследних
			ПО Задание_ат.Ссылка = ИсторияСтатусовЗаданий_атСрезПоследних.Ссылка
		ЛЕВОЕ СОЕДИНЕНИЕ (
			ВЫБРАТЬ
				Задание_ат.Ссылка КАК Ссылка,
				МАКСИМУМ(ВЫБОР
					КОГДА РодительскиеЗадания.Предок ЕСТЬ NULL 
							ИЛИ ИсторияСтатусовРодительскихЗаданий.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаданий_ат.Выполнено)
							ИЛИ ИсторияСтатусовРодительскихЗаданий.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаданий_ат.Прервано)
							ИЛИ ИсторияСтатусовРодительскихЗаданий.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаданий_ат.Отменено)
						ТОГДА ИСТИНА
						ИНАЧЕ ЛОЖЬ
					КОНЕЦ) КАК РодительскоеЗаданиеЗакрыто
			ИЗ
				Документ.Задание_ат КАК Задание_ат
					ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.СвязиОбъектов_ат КАК РодительскиеЗадания
						ПО Задание_ат.Ссылка = РодительскиеЗадания.Объект
							И (РодительскиеЗадания.СсылкаНаКод = 0)
							И (РодительскиеЗадания.Предок ССЫЛКА Документ.Задание_ат)
							И (НЕ РодительскиеЗадания.Предок.ПометкаУдаления)
					ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ИсторияСтатусовЗаданий_ат.СрезПоследних КАК ИсторияСтатусовРодительскихЗаданий
						ПО (РодительскиеЗадания.Предок = ИсторияСтатусовРодительскихЗаданий.Ссылка)
			СГРУППИРОВАТЬ ПО
				Задание_ат.Ссылка) КАК ИсторияСтатусовРодительскихЗаданий
			ПО Задание_ат.Ссылка = ИсторияСтатусовРодительскихЗаданий.Ссылка
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ИсторияКомментариев_ат.СрезПоследних КАК ИсторияКомментариев_атСрезПоследних
			ПО ИсторияКомментариев_атСрезПоследних.Ссылка = Задание_ат.Ссылка
ГДЕ
	ВЫБОР
		КОГДА &amp;ОтбиратьПоЗаявке
			ТОГДА
				РодительскиеЗаявки.Предок = &amp;Заявка
			ИНАЧЕ
				ИСТИНА
		КОНЕЦ
	
	И ВЫБОР
		КОГДА &amp;СкрытьПодчиненные
			ТОГДА
				ИсторияСтатусовРодительскихЗаданий.РодительскоеЗаданиеЗакрыто
			ИНАЧЕ
				ИСТИНА
		КОНЕЦ
	
	И ВЫБОР
		КОГДА &amp;ОтбиратьПоПользователю
			ТОГДА
				СвойстваЗаданий_ат.Исполнитель = &amp;ПользовательФильтраПерсональные
					ИЛИ СвойстваЗаданий_ат.Исполнитель = ЗНАЧЕНИЕ(Справочник.Пользователи.ПустаяСсылка)
					ИЛИ Задание_ат.Автор = &amp;ПользовательФильтраПерсональные
			ИНАЧЕ
				ИСТИНА
		КОНЕЦ
	
	И ВЫБОР
		КОГДА &amp;ТолькоАктуальные
			ТОГДА 
				ИСТИНА //???
			ИНАЧЕ
				ИСТИНА
		КОНЕЦ
</queryText>
      <mainTable>Document.Задание_ат</mainTable>
      <dynamicDataRead>true</dynamicDataRead>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
