  <attributes>
    <name>СписокЗаявок</name>
    <title>
      <key>ru</key>
      <value>Список заявок</value>
    </title>
    <id>1</id>
    <valueType>
      <types>DynamicList</types>
    </valueType>
    <view>
      <common>true</common>
    </view>
    <edit>
      <common>true</common>
    </edit>
    <notDefaultUseAlwaysAttributes xsi:type="form:MultiLanguageDataPath">
      <paths xsi:type="form:DataPath">
        <segments>СписокЗаявок.Ref</segments>
      </paths>
    </notDefaultUseAlwaysAttributes>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ РАЗЛИЧНЫЕ
	ДокументЗаявка.Ссылка КАК Ссылка,
	ДокументЗаявка.РодительскаяЗаявка,
	ИсторияСтатусовРодительскихЗаявок.Статус КАК СтатусРодительскойЗаявки,
	ДокументЗаявка.Дата,
	ДокументЗаявка.Тикет,
	ДокументЗаявка.Номер,
	ДокументЗаявка.Клиент,
	ДокументЗаявка.Подразделение,
	ДокументЗаявка.Сотрудник,
	ДокументЗаявка.Тезис,
	ДокументЗаявка.ТипЗаявки,
	ИсторияОжидаемойДатыЗавершенияРабот.ОжидаемаяДатаЗавершения,
	ДокументЗаявка.Проект,
	ДокументЗаявка.Ответственный,
	ЕСТЬNULL(ПриоритетЗаявок.Приоритет, 0) КАК Приоритет,
	ИсторияСтатусовЗаявок.Статус,
	ИсторияСтатусовЗаявок.Период КАК ДатаСтатуса,
	ФинансовыеСтатусыРабот_ат.Статус КАК ФинансовыйСтатус,
	ВЫБОР
		КОГДА ДокументЗаявка.Ответственный = &amp;ПользовательФильтраПерсональные
			ТОГДА
				ИСТИНА
			ИНАЧЕ
				ЛОЖЬ
		КОНЕЦ КАК ЯОтветственный,
	ВЫБОР
		КОГДА ИсторияСтатусовЗаявок.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаявок_ат.Выполняется)
			ТОГДА 0
		КОГДА ИсторияСтатусовЗаявок.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаявок_ат.Зарегистрирована)
			ИЛИ ИсторияСтатусовЗаявок.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаявок_ат.НаРассмотрении)
			ТОГДА 1
		ИНАЧЕ 2
	КОНЕЦ КАК ПриоритетПоСтатусу,
	ВЫБОР
		КОГДА ИсторияСтатусовЗаявок.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаявок_ат.Зарегистрирована)
				ИЛИ ИсторияСтатусовЗаявок.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаявок_ат.НаРассмотрении)
			ТОГДА ЕСТЬNULL(ПриоритетЗаявок.Приоритет, 0)
		КОГДА ИсторияСтатусовЗаявок.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаявок_ат.ВОчереди)
			И ЕСТЬNULL(СвойстваЗаявокПоИсполнителям.ПланируемоеНачалоРабот, ДАТАВРЕМЯ(1, 1, 1)) &lt;> ДАТАВРЕМЯ(1, 1, 1)
			ТОГДА РАЗНОСТЬДАТ(&amp;ТекущаяДата, СвойстваЗаявокПоИсполнителям.ПланируемоеНачалоРабот, СЕКУНДА)
		КОГДА ИсторияСтатусовЗаявок.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаявок_ат.Выполняется)
			И ЕСТЬNULL(ИсторияОжидаемойДатыЗавершенияРабот.ОжидаемаяДатаЗавершения, ДАТАВРЕМЯ(1, 1, 1)) &lt;> ДАТАВРЕМЯ(1, 1, 1)
			ТОГДА РАЗНОСТЬДАТ(&amp;ТекущаяДата, ИсторияОжидаемойДатыЗавершенияРабот.ОжидаемаяДатаЗавершения, СЕКУНДА)
		КОГДА ЕСТЬNULL(ИсторияСтатусовЗаявок.Период, ДАТАВРЕМЯ(1, 1, 1)) &lt;> ДАТАВРЕМЯ(1, 1, 1)
			ТОГДА РАЗНОСТЬДАТ(&amp;ТекущаяДата, ИсторияСтатусовЗаявок.Период, СЕКУНДА) // ДатаСтатуса
		ИНАЧЕ РАЗНОСТЬДАТ(&amp;ТекущаяДата, ДокументЗаявка.Дата, СЕКУНДА)
	КОНЕЦ КАК ЗначениеСортировки,
	//ВЫБОР
	//	КОГДА ИсторияСтатусовЗаявок.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаявок_ат.ВОчереди)
	//			ИЛИ ИсторияСтатусовЗаявок.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаявок_ат.Выполняется)
	//		ТОГДА
	//			ИСТИНА
	//		ИНАЧЕ
	//			ЛОЖЬ
	//	КОНЕЦ КАК Выполняется,
	//ВЫБОР
	//	КОГДА ИсторияСтатусовЗаявок.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаявок_ат.ВОчереди)
	//		ТОГДА
	//			СвойстваЗаявок.ПланируемаяДатаНачала
	//		ИНАЧЕ
	//			ВЫБОР
	//				КОГДА ИсторияСтатусовЗаявок.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаявок_ат.Выполняется)
	//						И ДокументЗаявка.ОжидаемаяДатаОкончания &lt;> ДАТАВРЕМЯ(1, 1, 1)
	//					ТОГДА
	//						ДокументЗаявка.ОжидаемаяДатаОкончания
	//					ИНАЧЕ
	//						ИсторияСтатусовЗаявок.Период
	//				КОНЕЦ
	//	КОНЕЦ КАК ПриоритетПоДате
	ВЫБОР
		КОГДА ДокументЗаявка.ТипЗаявки.Исправление
			ТОГДА
				ВЫБОР
					КОГДА ДокументЗаявка.ТипЗаявки.Срочное
						ТОГДА
							0
						ИНАЧЕ
							50
					КОНЕЦ
			ИНАЧЕ
				ВЫБОР
					КОГДА ДокументЗаявка.ТипЗаявки.Срочное
						ТОГДА
							100
						ИНАЧЕ
							500
					КОНЕЦ
		КОНЕЦ КАК ПриоритетПоТипуЗаявки,
	ЗНАЧЕНИЕ(Документ.Релиз_ат.ПустаяСсылка) КАК Релиз,
	ВЫБОР
		КОГДА СвойстваЗаявокПоИсполнителям.ПланируемоеНачалоРабот &lt;> ДАТАВРЕМЯ(1, 1, 1)
			ТОГДА
				СвойстваЗаявокПоИсполнителям.ПланируемоеНачалоРабот
			ИНАЧЕ
				ВЫБОР
					КОГДА ИсторияСтатусовЗаявок.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаявок_ат.НаРассмотрении)
						ТОГДА
							ВЫБОР
								КОГДА ЕСТЬNULL(ВременаРаботОбороты.ПланируемоеВремяОборот, 0) = 0
									ТОГДА 0
									ИНАЧЕ (ВЫРАЗИТЬ(ВременаРаботОбороты.ПланируемоеВремяОборот / 60 - 0.5 КАК ЧИСЛО(8, 0)))
										+ (ВременаРаботОбороты.ПланируемоеВремяОборот - (ВЫРАЗИТЬ(ВременаРаботОбороты.ПланируемоеВремяОборот / 60 - 0.5 КАК ЧИСЛО(8, 0))) * 60) / 100
								КОНЕЦ
						ИНАЧЕ
							ВЫБОР
								КОГДА ИсторияСтатусовЗаявок.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаявок_ат.ВОчереди)
										ИЛИ ИсторияСтатусовЗаявок.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаявок_ат.Выполняется)
									ТОГДА
										ВЫБОР
											КОГДА ИсторияОжидаемойДатыЗавершенияРабот.ОжидаемаяДатаЗавершения &lt;> ДАТАВРЕМЯ(1, 1, 1)
												ТОГДА
													РАЗНОСТЬДАТ(&amp;ТекущаяДата, ИсторияОжидаемойДатыЗавершенияРабот.ОжидаемаяДатаЗавершения, ДЕНЬ)
												ИНАЧЕ
													ВЫБОР
														КОГДА СвойстваЗаявок.ПланируемаяДатаОкончания &lt;> ДАТАВРЕМЯ(1, 1, 1)
															ТОГДА
																РАЗНОСТЬДАТ(&amp;ТекущаяДата, СвойстваЗаявок.ПланируемаяДатаОкончания, ДЕНЬ)
															ИНАЧЕ
																0
														КОНЕЦ
											КОНЕЦ
									ИНАЧЕ
										РАЗНОСТЬДАТ(&amp;ТекущаяДата, ИсторияСтатусовЗаявок.Период, ДЕНЬ) // ДатаСтатуса
								КОНЕЦ
					КОНЕЦ
		КОНЕЦ КАК ДопИнфо
ИЗ
	Документ.Заявка_ат КАК ДокументЗаявка
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.СвойстваЗаявок_ат КАК СвойстваЗаявок
			ПО ДокументЗаявка.Ссылка = СвойстваЗаявок.Ссылка
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ИсторияОжидаемойДатыЗавершенияРабот_ат.СрезПоследних КАК ИсторияОжидаемойДатыЗавершенияРабот
			ПО ДокументЗаявка.Ссылка = ИсторияОжидаемойДатыЗавершенияРабот.Заявка
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ПриоритетЗаявок_ат КАК ПриоритетЗаявок
			ПО ДокументЗаявка.Ссылка = ПриоритетЗаявок.Заявка
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ИсторияСтатусовЗаявок_ат.СрезПоследних КАК ИсторияСтатусовЗаявок
			ПО ДокументЗаявка.Ссылка = ИсторияСтатусовЗаявок.Ссылка
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ИсторияСтатусовЗаявок_ат.СрезПоследних КАК ИсторияСтатусовРодительскихЗаявок
			ПО ДокументЗаявка.РодительскаяЗаявка.Ссылка = ИсторияСтатусовРодительскихЗаявок.Ссылка
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.СвязиОбъектов_ат КАК СвязиОбъектовЗаданий
			ПО ДокументЗаявка.Ссылка = СвязиОбъектовЗаданий.Предок
				И СвязиОбъектовЗаданий.Объект ССЫЛКА Документ.Задание_ат
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ФинансовыеСтатусыРабот_ат.СрезПоследних КАК ФинансовыеСтатусыРабот_ат
			ПО ДокументЗаявка.Ссылка = ФинансовыеСтатусыРабот_ат.Ссылка
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.ВременаРабот_ат.Обороты КАК ВременаРаботОбороты
			ПО ДокументЗаявка.Ссылка = ВременаРаботОбороты.Заявка
		ЛЕВОЕ СОЕДИНЕНИЕ
			(ВЫБРАТЬ
				СвойстваЗаявокПоИсполнителям.Ссылка КАК Заявка,
				МИНИМУМ(СвойстваЗаявокПоИсполнителям.ПланируемоеНачалоРабот) КАК ПланируемоеНачалоРабот
			ИЗ
				РегистрСведений.СвойстваЗаявокПоИсполнителям_ат КАК СвойстваЗаявокПоИсполнителям
			ГДЕ
				ВЫБОР
					КОГДА &amp;ОтбиратьПоПользователю
						ТОГДА
							СвойстваЗаявокПоИсполнителям.Исполнитель = &amp;ПользовательФильтраПерсональные
						ИНАЧЕ
							ИСТИНА
					КОНЕЦ
			СГРУППИРОВАТЬ ПО
				СвойстваЗаявокПоИсполнителям.Ссылка
			) КАК СвойстваЗаявокПоИсполнителям
			ПО ДокументЗаявка.Ссылка = СвойстваЗаявокПоИсполнителям.Заявка
ГДЕ
	ВЫБОР
		КОГДА &amp;ОтбиратьПоЗаданию
			ТОГДА
				СвязиОбъектовЗаданий.Объект = &amp;Задание
			ИНАЧЕ
				ИСТИНА
		КОНЕЦ
	
	И ВЫБОР
		КОГДА &amp;СкрытьПодчиненные
			ТОГДА
				ДокументЗаявка.РодительскаяЗаявка = ЗНАЧЕНИЕ(Документ.Заявка_ат.ПустаяСсылка)
				ИЛИ ДокументЗаявка.РодительскаяЗаявка.ПометкаУдаления
				ИЛИ ИсторияСтатусовРодительскихЗаявок.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаявок_ат.Закрыта)
				ИЛИ ИсторияСтатусовРодительскихЗаявок.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаявок_ат.Отменена)
				ИЛИ ИсторияСтатусовРодительскихЗаявок.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаявок_ат.Отклонена)
			ИНАЧЕ
				ИСТИНА
		КОНЕЦ
	
	И ВЫБОР
		КОГДА &amp;ОтбиратьПоПользователю
			ТОГДА
				ДокументЗаявка.Сотрудник = &amp;ПользовательФильтраПерсональные
				ИЛИ ДокументЗаявка.Ответственный = &amp;ПользовательФильтраПерсональные
				ИЛИ 1 В // отбор по Заданиям
					(ВЫБРАТЬ ПЕРВЫЕ 1
						1
					ИЗ
						РегистрСведений.СвязиОбъектов_ат КАК СвязиОбъектов_ат
							ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.СвойстваЗаданий_ат КАК СвойстваЗаданий_ат
								ПО СвойстваЗаданий_ат.Ссылка = СвязиОбъектов_ат.Объект
							ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ИсторияСтатусовЗаданий_ат.СрезПоследних(,) КАК ИсторияСтатусовЗаданий_атСрезПоследних
								ПО ИсторияСтатусовЗаданий_атСрезПоследних.Ссылка = СвязиОбъектов_ат.Объект
					ГДЕ
						СвязиОбъектов_ат.Предок = ДокументЗаявка.Ссылка
						И СвязиОбъектов_ат.Объект ССЫЛКА Документ.Задание_ат
						И НЕ СвязиОбъектов_ат.Объект.ПометкаУдаления
						И (СвойстваЗаданий_ат.Исполнитель = &amp;ПользовательФильтраПерсональные
							ИЛИ СвязиОбъектов_ат.Объект.Автор = &amp;ПользовательФильтраПерсональные)
						И ВЫБОР
							КОГДА &amp;ТолькоАктуальные
								ТОГДА
									(ИсторияСтатусовЗаданий_атСрезПоследних.Статус ЕСТЬ NULL 
									ИЛИ (НЕ ИсторияСтатусовЗаданий_атСрезПоследних.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаданий_ат.Выполнено)
										И НЕ ИсторияСтатусовЗаданий_атСрезПоследних.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаданий_ат.Прервано)
										И НЕ ИсторияСтатусовЗаданий_атСрезПоследних.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаданий_ат.Отменено)))
								ИНАЧЕ
									ИСТИНА
							КОНЕЦ
					)
				//ИЛИ 1 В
				//	(ВЫБРАТЬ ПЕРВЫЕ 1
				//		1
				//	ИЗ
				//		РегистрСведений.УведомлятьИСогласовывать КАК УиС //TODO: переименовать в КонтрольОбслуживания
				//	ГДЕ
				//		УиС.Сотрудник = &amp;ПользовательФильтраПерсональные
				//		И (УиС.Уведомления ИЛИ УиС.ФинансовыеУведомления ИЛИ УиС.Согласования ИЛИ УиС.ФинансовыеСогласования) //TODO: использовать .ВидимостьЗаявок
				//		И ((УиС.Клиент = ДокументЗаявка.Клиент И УиС.Подразделение = ДокументЗаявка.Проект И УиС.Проект = ДокументЗаявка.Проект)
				//				ИЛИ (УиС.Клиент = ДокументЗаявка.Клиент
				//					И УиС.Подразделение = ДокументЗаявка.Подразделение
				//					И УиС.Проект = ЗНАЧЕНИЕ(Справочник.Проекты_ат.ПустаяСсылка))
				//				ИЛИ (УиС.Клиент = ДокументЗаявка.Клиент
				//					И УиС.Подразделение = ЗНАЧЕНИЕ(Справочник.Подразделения_ат.ПустаяСсылка)
				//					И УиС.Проект = ДокументЗаявка.Проект)
				//				ИЛИ (УиС.Клиент = ДокументЗаявка.Клиент
				//					И УиС.Подразделение = ЗНАЧЕНИЕ(Справочник.Подразделения_ат.ПустаяСсылка)
				//					И УиС.Проект = ЗНАЧЕНИЕ(Справочник.Проекты_ат.ПустаяСсылка)))
				//	)
			ИНАЧЕ
				ИСТИНА
		КОНЕЦ
	
	И ВЫБОР
		КОГДА &amp;ТолькоАктуальные
			ТОГДА
				ДокументЗаявка.Автор = &amp;ПользовательФильтраПерсональные
				ИЛИ ДокументЗаявка.Сотрудник = &amp;ПользовательФильтраПерсональные
				ИЛИ ДокументЗаявка.Ответственный = &amp;ПользовательФильтраПерсональные
				ИЛИ ДокументЗаявка.Ответственный = ЗНАЧЕНИЕ(Справочник.Пользователи.ПустаяСсылка)
				ИЛИ ДокументЗаявка.Проект.Ответственный = &amp;ПользовательФильтраПерсональные
				
				ИЛИ 1 В // отбор по Заданиям
					(ВЫБРАТЬ ПЕРВЫЕ 1
						1
					ИЗ
						РегистрСведений.СвязиОбъектов_ат КАК СвязиОбъектов_ат
							ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.СвойстваЗаданий_ат КАК СвойстваЗаданий_ат
								ПО СвойстваЗаданий_ат.Ссылка = СвязиОбъектов_ат.Объект
							ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ИсторияСтатусовЗаданий_ат.СрезПоследних(, ) КАК ИсторияСтатусовЗаданий_атСрезПоследних
								ПО ИсторияСтатусовЗаданий_атСрезПоследних.Ссылка = СвязиОбъектов_ат.Объект
					ГДЕ
						СвязиОбъектов_ат.Предок = ДокументЗаявка.Ссылка
						И СвязиОбъектов_ат.Объект ССЫЛКА Документ.Задание_ат
						И НЕ СвязиОбъектов_ат.Объект.ПометкаУдаления
						И (СвойстваЗаданий_ат.Исполнитель = &amp;ПользовательФильтраПерсональные
							ИЛИ СвойстваЗаданий_ат.Исполнитель = ЗНАЧЕНИЕ(Справочник.Пользователи.ПустаяСсылка)
							ИЛИ СвязиОбъектов_ат.Объект.Автор = &amp;ПользовательФильтраПерсональные)
						И (ИсторияСтатусовЗаданий_атСрезПоследних.Статус ЕСТЬ NULL 
							ИЛИ (НЕ ИсторияСтатусовЗаданий_атСрезПоследних.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаданий_ат.Выполнено)
								И НЕ ИсторияСтатусовЗаданий_атСрезПоследних.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаданий_ат.Прервано)
								И НЕ ИсторияСтатусовЗаданий_атСрезПоследних.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаданий_ат.Отменено)))
					)
				
				ИЛИ 1 В // отбор по Кураторам Проектов
					(ВЫБРАТЬ ПЕРВЫЕ 1
						1
					ИЗ
						Справочник.Проекты_ат.Кураторы КАК ПроектКураторы
					ГДЕ
						ПроектКураторы.Ссылка = ДокументЗаявка.Проект
						И ПроектКураторы.Куратор = &amp;ПользовательФильтраПерсональные)
				ИЛИ (//ДокументЗаявка.Проект.Ответственный = ЗНАЧЕНИЕ(Справочник.Пользователи.ПустаяСсылка) И
					НЕ 1 В // отсутствуют Кураторы Проектов
						(ВЫБРАТЬ ПЕРВЫЕ 1
							1
						ИЗ
							Справочник.Проекты_ат.Кураторы КАК Проекты_атКураторы_Заполнены
						ГДЕ
							Проекты_атКураторы_Заполнены.Ссылка = ДокументЗаявка.Проект
							И Проекты_атКураторы_Заполнены.НомерСтроки = 1
						)
					)
			ИНАЧЕ
				ИСТИНА
		КОНЕЦ
</queryText>
      <mainTable>Document.Заявка_ат</mainTable>
