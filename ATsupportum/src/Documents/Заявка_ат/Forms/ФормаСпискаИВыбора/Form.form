    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ДокументЗаявка.Ссылка КАК Ссылка,
	ДокументЗаявка.РодительскаяЗаявка КАК РодительскаяЗаявка,
	ИсторияСтатусовРодительскихЗаявокСрезПоследних.Статус КАК СтатусРодительскойЗаявки,
	ДокументЗаявка.ПометкаУдаления КАК ПометкаУдаления,
	ДокументЗаявка.Тикет КАК Тикет,
	ДокументЗаявка.Номер КАК Номер,
	ДокументЗаявка.Дата КАК Дата,
	ДокументЗаявка.Клиент КАК Клиент,
	ДокументЗаявка.Подразделение КАК Подразделение,
	ДокументЗаявка.Сотрудник КАК Сотрудник,
	ВЫБОР
		КОГДА НЕ &amp;СотрудникОрганизаций
				И ДокументЗаявка.ТипЗаявки.ОтображениеКлиенту &lt;> ЗНАЧЕНИЕ(Справочник.ТипыЗаявок_ат.ПустаяСсылка)
			ТОГДА ДокументЗаявка.ТипЗаявки.ОтображениеКлиенту
		ИНАЧЕ ДокументЗаявка.ТипЗаявки
	КОНЕЦ КАК ТипЗаявки,
	ДокументЗаявка.Тезис КАК Тезис,
	ДокументЗаявка.ТекстЗаявки КАК ТекстЗаявки,
	ДокументЗаявка.Проект КАК Проект,
	ДокументЗаявка.Ответственный КАК Ответственный,
	ЕСТЬNULL(ИсторияОДЗСрезПоследних.ОжидаемаяДатаЗавершения, ДАТАВРЕМЯ(1, 1, 1)) КАК ОжидаемаяДатаЗавершения,
	ЕСТЬNULL(ИсторияСтатусовЗаявокСрезПоследних.Период, ДАТАВРЕМЯ(1, 1, 1)) КАК ДатаСтатуса,
	ВЫБОР
		КОГДА ДокументЗаявка.Ответственный = &amp;ТекущийПользователь
			ТОГДА ИСТИНА
		ИНАЧЕ ЛОЖЬ
	КОНЕЦ КАК ОтветственныйЭтоТекущийПользователь,
	ВЫБОР
		КОГДА &amp;СотрудникОрганизаций
			ТОГДА ИсторияСтатусовЗаявокСрезПоследних.Статус
		ИНАЧЕ ВЫБОР
				КОГДА ИсторияСтатусовЗаявокСрезПоследних.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаявок_ат.ВОчереди)
					ТОГДА ЗНАЧЕНИЕ(Перечисление.СтатусыЗаявок_ат.Выполняется)
				ИНАЧЕ ИсторияСтатусовЗаявокСрезПоследних.Статус
			КОНЕЦ
	КОНЕЦ КАК Статус,
	ВЫБОР
		КОГДА &amp;СотрудникОрганизаций
			ТОГДА ВЫБОР
					КОГДА ИсторияСтатусовЗаявокСрезПоследних.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаявок_ат.ВОчереди)
							ИЛИ ИсторияСтатусовЗаявокСрезПоследних.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаявок_ат.Выполняется)
							ИЛИ ИсторияСтатусовЗаявокСрезПоследних.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаявок_ат.НаТестировании)
						ТОГДА ИСТИНА
					ИНАЧЕ ЛОЖЬ
				КОНЕЦ
		ИНАЧЕ ИСТИНА
	КОНЕЦ КАК Выполняется,
	ВЫБОР
		КОГДА &amp;СотрудникОрганизаций
			ТОГДА ВЫБОР
				КОГДА ИсторияСтатусовЗаявокСрезПоследних.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаявок_ат.Выполняется)
					ТОГДА 0
				КОГДА ИсторияСтатусовЗаявокСрезПоследних.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаявок_ат.Зарегистрирована)
					ИЛИ ИсторияСтатусовЗаявокСрезПоследних.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаявок_ат.НаРассмотрении)
					ТОГДА 1
				ИНАЧЕ 2
				КОНЕЦ
		ИНАЧЕ 0
	КОНЕЦ КАК ПриоритетПоСтатусу,
	ВЫБОР
		КОГДА ИсторияСтатусовЗаявокСрезПоследних.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаявок_ат.Зарегистрирована)
				ИЛИ ИсторияСтатусовЗаявокСрезПоследних.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаявок_ат.НаРассмотрении)
			ТОГДА ЕСТЬNULL(ПриоритетЗаявок.Приоритет, 0)
		ИНАЧЕ ВЫБОР
			КОГДА &amp;СотрудникОрганизаций
				ТОГДА ВЫБОР
					КОГДА ИсторияСтатусовЗаявокСрезПоследних.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаявок_ат.Зарегистрирована)
						ИЛИ ИсторияСтатусовЗаявокСрезПоследних.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаявок_ат.НаРассмотрении)
						ТОГДА ЕСТЬNULL(ПриоритетЗаявок.Приоритет, 0)
					КОГДА ИсторияСтатусовЗаявокСрезПоследних.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаявок_ат.ВОчереди)
						И ЕСТЬNULL(СвойстваЗаявокПоИсполнителям.ПланируемоеНачалоРабот, ДАТАВРЕМЯ(1, 1, 1)) &lt;> ДАТАВРЕМЯ(1, 1, 1)
						ТОГДА РАЗНОСТЬДАТ(&amp;ТекущаяДата, СвойстваЗаявокПоИсполнителям.ПланируемоеНачалоРабот, СЕКУНДА)
					КОГДА ИсторияСтатусовЗаявокСрезПоследних.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаявок_ат.Выполняется)
						И ЕСТЬNULL(ИсторияОДЗСрезПоследних.ОжидаемаяДатаЗавершения, ДАТАВРЕМЯ(1, 1, 1)) &lt;> ДАТАВРЕМЯ(1, 1, 1)
						ТОГДА РАЗНОСТЬДАТ(&amp;ТекущаяДата, ИсторияОДЗСрезПоследних.ОжидаемаяДатаЗавершения, СЕКУНДА)
					КОГДА ЕСТЬNULL(ИсторияСтатусовЗаявокСрезПоследних.Период, ДАТАВРЕМЯ(1, 1, 1)) &lt;> ДАТАВРЕМЯ(1, 1, 1)
						ТОГДА РАЗНОСТЬДАТ(&amp;ТекущаяДата, ИсторияСтатусовЗаявокСрезПоследних.Период, СЕКУНДА) // ДатаСтатуса
					ИНАЧЕ РАЗНОСТЬДАТ(&amp;ТекущаяДата, ДокументЗаявка.Дата, СЕКУНДА)
				КОНЕЦ
			ИНАЧЕ РАЗНОСТЬДАТ(&amp;ТекущаяДата, ДокументЗаявка.Дата, СЕКУНДА)
		КОНЕЦ
	КОНЕЦ КАК ЗначениеСортировки,
	ЕСТЬNULL(СвойстваЗаявок.ПланируемаяДатаНачала, ДАТАВРЕМЯ(1, 1, 1)) КАК ПланируемаяДатаНачала,
	ЕСТЬNULL(СвойстваЗаявок.ПланируемаяДатаОкончания, ДАТАВРЕМЯ(1, 1, 1)) КАК ПланируемаяДатаОкончания,
	ЕСТЬNULL(СвойстваЗаявок.ПланируемаяДлительность, 0) КАК ПланируемаяДлительность,
	ВЫБОР
		КОГДА ЕСТЬNULL(ВременаРаботОбороты.ПланируемоеВремяОборот, 0) = 0
			ТОГДА 0
		ИНАЧЕ (ВЫРАЗИТЬ(ВременаРаботОбороты.ПланируемоеВремяОборот / 60 - 0.5 КАК ЧИСЛО(8, 0))) + (ВременаРаботОбороты.ПланируемоеВремяОборот - (ВЫРАЗИТЬ(ВременаРаботОбороты.ПланируемоеВремяОборот / 60 - 0.5 КАК ЧИСЛО(8, 0))) * 60) / 100
	КОНЕЦ КАК ПланируемоеВремяВыполнения,
	ЕСТЬNULL(СвойстваЗаявок.ФактическаяДатаНачала, ДАТАВРЕМЯ(1, 1, 1)) КАК ФактическаяДатаНачала,
	ЕСТЬNULL(СвойстваЗаявок.ФактическаяДатаОкончания, ДАТАВРЕМЯ(1, 1, 1)) КАК ФактическаяДатаОкончания,
	ЕСТЬNULL(СвойстваЗаявок.ФактическаяДлительность, 0) КАК ФактическаяДлительность,
	ВЫБОР
		КОГДА ЕСТЬNULL(ВременаРаботОбороты.ФактическоеВремяОборот, 0) = 0
			ТОГДА 0
		ИНАЧЕ (ВЫРАЗИТЬ(ВременаРаботОбороты.ФактическоеВремяОборот / 60 - 0.5 КАК ЧИСЛО(8, 0))) + (ВременаРаботОбороты.ФактическоеВремяОборот - (ВЫРАЗИТЬ(ВременаРаботОбороты.ФактическоеВремяОборот / 60 - 0.5 КАК ЧИСЛО(8, 0))) * 60) / 100
	КОНЕЦ КАК ФактическоеВремяВыполнения,
	ЕСТЬNULL(СвойстваЗаявок.ОтчетнаяДатаНачала, ДАТАВРЕМЯ(1, 1, 1)) КАК ОтчетнаяДатаНачала,
	ЕСТЬNULL(СвойстваЗаявок.ОтчетнаяДатаОкончания, ДАТАВРЕМЯ(1, 1, 1)) КАК ОтчетнаяДатаОкончания,
	ЕСТЬNULL(СвойстваЗаявок.ОтчетнаяДлительность, 0) КАК ОтчетнаяДлительность,
	ВЫБОР
		КОГДА ЕСТЬNULL(ВременаРаботОбороты.ОтчетноеВремяОборот, 0) = 0
			ТОГДА 0
		ИНАЧЕ (ВЫРАЗИТЬ(ВременаРаботОбороты.ОтчетноеВремяОборот / 60 - 0.5 КАК ЧИСЛО(8, 0))) + (ВременаРаботОбороты.ОтчетноеВремяОборот - (ВЫРАЗИТЬ(ВременаРаботОбороты.ОтчетноеВремяОборот / 60 - 0.5 КАК ЧИСЛО(8, 0))) * 60) / 100
	КОНЕЦ КАК ОтчетноеВремяВыполнения,
	ВЫБОР
		КОГДА ЕСТЬNULL(ВременаРаботОбороты.СогласованноеВремяОборот, 0) = 0
			ТОГДА 0
		ИНАЧЕ (ВЫРАЗИТЬ(ВременаРаботОбороты.СогласованноеВремяОборот / 60 - 0.5 КАК ЧИСЛО(8, 0))) + (ВременаРаботОбороты.СогласованноеВремяОборот - (ВЫРАЗИТЬ(ВременаРаботОбороты.СогласованноеВремяОборот / 60 - 0.5 КАК ЧИСЛО(8, 0))) * 60) / 100
	КОНЕЦ КАК СогласованноеВремяВыполнения,
	ЕСТЬNULL(СвойстваЗаявок.РезультатВыполнения, &quot;&quot;) КАК РезультатВыполнения,
	ВЫБОР
		КОГДА ВременаРаботОбороты.ПланируемоеВремяОборот ЕСТЬ NULL
			ТОГДА 0
		КОГДА ВременаРаботОбороты.ПланируемоеВремяОборот - ВременаРаботОбороты.ОтчетноеВремяОборот = 0
			ТОГДА 0
		ИНАЧЕ ВЫБОР
				КОГДА ВременаРаботОбороты.ПланируемоеВремяОборот - ВременаРаботОбороты.ОтчетноеВремяОборот > 0
					ТОГДА (ВЫРАЗИТЬ((ВременаРаботОбороты.ПланируемоеВремяОборот - ВременаРаботОбороты.ОтчетноеВремяОборот) / 60 - 0.5 КАК ЧИСЛО(8, 0))) + (ВременаРаботОбороты.ПланируемоеВремяОборот - ВременаРаботОбороты.ОтчетноеВремяОборот - (ВЫРАЗИТЬ((ВременаРаботОбороты.ПланируемоеВремяОборот - ВременаРаботОбороты.ОтчетноеВремяОборот) / 60 - 0.5 КАК ЧИСЛО(8, 0))) * 60) / 100
				ИНАЧЕ (ВЫРАЗИТЬ((ВременаРаботОбороты.ПланируемоеВремяОборот - ВременаРаботОбороты.ОтчетноеВремяОборот) / 60 + 0.5 КАК ЧИСЛО(8, 0))) + (ВременаРаботОбороты.ПланируемоеВремяОборот - ВременаРаботОбороты.ОтчетноеВремяОборот - (ВЫРАЗИТЬ((ВременаРаботОбороты.ПланируемоеВремяОборот - ВременаРаботОбороты.ОтчетноеВремяОборот) / 60 + 0.5 КАК ЧИСЛО(8, 0))) * 60) / 100
			КОНЕЦ
	КОНЕЦ КАК ПланируемоеМинусОтчетноеВремя,
	ВЫБОР
		КОГДА ВременаРаботОбороты.ПланируемоеВремяОборот ЕСТЬ NULL
			ТОГДА 0
